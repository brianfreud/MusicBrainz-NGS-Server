[% WRAPPER "search/error/wrapper.tt" %]
    <p>
        [% l('Sorry, your query (<code>[_1]</code>) does not contain enough information to carry out a search. If you are attempting to find something that contains nothing but punctuation characters (e.g. !!!) then you need to use the <a href=\"[_2]\">browse page</a>, since this search ignores punctuation characters.', html_escape(query), c.uri_for("/${type}/browse")) %]
    </p>
[% END %]
