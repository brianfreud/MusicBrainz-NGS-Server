[%- MACRO tag_list(tags, entity) BLOCK;
    tag_links = [];
    FOR tag=tags;
        tag_links.push(link_tag(tag.tag));
    END;
    tag_links.push(link_entity(entity, 'tags', l('more&hellip;')));
    tag_links.join(', ');
END -%]
<div class="datumGroup">
    <dl class="datumList">
        <dt class="exp datumLabels">
            [% l('Tags:') %]
        </dt>
        <dd class="exp">
            [% tag_list(tags, entity) %]
        </dd>
    </dl>
</div>
